{
  "expo": {
    "name": "Mars Explorer",
    "slug": "mars-explorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.uvkal_za.marsexplorer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "An app where you can view all the latest images captured from all the Rovers that have or are on Mars."
  }
}
